fn fib(n: i64) i64 {
    if (n <= 1) { return n }
    return fib(n - 1) + fib(n - 2)
}

fn sum_to(n: i64) i64 {
    var total = 0
    var i = 0
    while (i < n) {
        total = total + i
        i = i + 1
    }
    return total
}

bench "fibonacci 20" {
    fib(20)
}

bench "sum to 1000" {
    sum_to(1000)
}

bench "empty" {
    var x = 0
    x = x + 1
}
