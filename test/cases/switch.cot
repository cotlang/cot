enum CswLevel {
    Low = 10,
    Medium = 50,
    High = 100
}

test "switch simple" {
    var x: i64 = 2
    var result = switch x {
        1 => 10,
        2 => 20,
        3 => 30,
        else => 0,
    }
    @assert_eq(result, 20)
}

test "switch enum" {
    var lvl = CswLevel.Medium
    var val = switch lvl {
        CswLevel.Low => 1,
        CswLevel.Medium => 50,
        CswLevel.High => 99,
        else => 0,
    }
    @assert_eq(val, 50)
}
